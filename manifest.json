{
  "manifest_version": 3,
  "name": "DOM Saver",
  "version": "1.0",
  "description": "Saves the DOM of the current page.",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["jszip.js"],
    "matches": ["<all_urls>"]
  }],
  "icons": {
  }
}